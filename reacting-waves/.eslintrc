
{
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["warn", "single"],
        "no-console": ["off"],
        "no-unused-vars": ["warn"],
        "no-undef":  ["warn"],
        "semi": ["warn", "always"]
    }
}
